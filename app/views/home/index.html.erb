<!-- Knot List Items - 50 circles, in rows of 5 -->
<div id="circle-list" align="center"> 

<% (1..50).each do |n| %>
  <%= link_to image_tag('circles/C' + n.to_s + '.jpg', :class => "circle"), circles_path %>
<% end %>

</div>
  
